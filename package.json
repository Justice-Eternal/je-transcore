{
  "name": "je-transcore",
  "version": "0.0.1",
  "description": "tool for je score transfering",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --copy-files",
    "prepublish": "npm run lint && npm run best",
    "best": "npm run build && npm run _test",
    "test": "node_modules/mocha/bin/mocha test/*.test.js",
    "lint": "node_modules/eslint/bin/eslint.js src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Justice-Eternal/je-transcore.git"
  },
  "keywords": [
    "je",
    "score",
    "transfer"
  ],
  "author": "means88",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Justice-Eternal/je-transcore/issues"
  },
  "homepage": "https://github.com/Justice-Eternal/je-transcore#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.9.0",
    "expect": "^21.2.1",
    "mocha": "^4.0.1"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "add-module-exports"
    ]
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "lex": "^1.7.9"
  }
}
